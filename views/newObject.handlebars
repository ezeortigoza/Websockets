<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>
<div class="my-5">
    <form id="objectForm">
        <label for="name">Nombre</label>
        <input id="name" name="name">
        <label for="precio">Precio</label>
        <input id="precio" name="precio">
        <label for="color">Color</label>
        <input id="color" name="color">
        <label for="image">Image</label>
        <input type="file" id="image" name="image">
        <input type="submit">
    </form>
</div>
 <h1 class="text-center">Productos a√±adidos</h1>
<table class="table table-bordered table-striped text-center mt-4" >
    {{#each object}}
 <thead>
    <tr id="form">
      <th id="nombre" scope="col">Nombre: {{this.name}}</th>
      <th id="price" scope="col">Precio: {{this.precio}}</th>
      <th id="color" scope="col">Color: {{this.color}}</th>
      <th id="img" scope="col"> Imagen: <img scope="row" src="http://localhost:8080/img/{{this.image}}"></th>
    </tr>
 </thead>
  <tbody>
    {{/each}}
</table>
<div class="mx-auto text-center">
    <h1 class="text-center">Chat</h1>
    <input class="mx-auto text-center" id="chatBox" type="chatBox">
     <br>
     <span  id="log"></span> <br> <span  id="log"></span>
</div>
<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="js/newObject.js"></script>